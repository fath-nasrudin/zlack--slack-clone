  },
  "homepage": "https://github.com/fath-nasrudin/zlack--slack-clone#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@webpack-cli/generators": "^2.5.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
